<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp">
  <Id>8fe80a54-2c1f-49b2-bf49-ab0fecb78999</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Workfront Inc.</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Add Note To Workfront" />
  <Description DefaultValue="This Outlook add-In allows to add new update to Workfront note thread using information from notification e-mail."/>
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="MailBox" MinVersion="1.1" />
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://localhost:44300/AppRead/Index.html"/>
        <RequestedHeight>380</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteMailbox</Permissions>
  <Rule xsi:type="RuleCollection" Mode="And">
    <Rule xsi:type="ItemHasRegularExpressionMatch"
          PropertyName="BodyAsHTML"
          RegExName="CommentOnProject"
          RegExValue="[\w-:./]+(?=/project/view\?ID=\w{32}&amp;amp;commentID=\w{32}&amp;amp;activeTab=tab-project-updates&amp;amp;showCommentBox=true)"
          IgnoreCase="true"/>
  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
</OfficeApp>
